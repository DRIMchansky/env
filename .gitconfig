[user]
  name  = Nikita Chernov
  email = drimchansky@gmail.com
[alias]
  st = status -sb
  lg = log --pretty=format:'%Cred%h%Creset%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%an%Creset' --abbrev-commit
  last = log -1 HEAD --stat
[color]
  diff   = auto
  status = auto
  branch = auto
[branch]
  autosetuprebase = always
[core]
  editor = code
[init]
  defaultBranch = main